@font-face {
    font-family: 'emprunt';
    src:    url('../fonts/emprunt.eot?ue0s55');
    src:    url('../fonts/emprunt.eot?ue0s55#iefix') format('embedded-opentype'),
            url('../fonts/emprunt.ttf?ue0s55') format('truetype'),
            url('../fonts/emprunt.woff?ue0s55') format('woff'),
            url('../fonts/emprunt.svg?ue0s55#icomoon') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="ef-"], [class*=" ef-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'emprunt' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.ef-jacket:before {
    content: "\e900";
}
.ef-mask:before {
    content: "\e901";
}
.ef-regulator:before {
    content: "\e902";
}
.ef-tank:before {
    content: "\e903";
}

